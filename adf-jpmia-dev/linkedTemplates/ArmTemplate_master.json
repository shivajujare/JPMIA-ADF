{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-jpmia-dev"
		},
		"ls_adls_jpmia_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsjpmiadev.dfs.core.windows.net/"
		},
		"ls_kv_jpmia_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-jpmia-dev.vault.azure.net/"
		},
		"ls_referentials_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sql-referential-con-str"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-jpmia-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_jpmia_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_jpmia_properties_typeProperties_url')]"
					},
					"ls_kv_jpmia_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_jpmia_properties_typeProperties_baseUrl')]"
					},
					"ls_referentials_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_referentials_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}